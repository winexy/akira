{
  "name": "akira",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "commit": "cz",
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "format:check": "prettier --check '**/*.{js,jsx,ts,tsx,json,css,md,yml}'",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,json,css,md,yml}'",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "release": "semantic-release",
    "_compile-action": "ncc build .github/actions/${npm_config_action}/index.js -o .github/actions/${npm_config_action}/dist",
    "compile-action": "npm run _compile-action"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/winexy/akira"
  },
  "dependencies": {
    "@heroicons/react": "^1.0.1",
